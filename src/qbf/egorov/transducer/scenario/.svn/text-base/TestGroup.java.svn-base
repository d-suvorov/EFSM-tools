package ru.ifmo.ctddev.genetic.transducer.scenario;

import java.util.List;

/**
 * @author kegorov
 *         Date: Jun 18, 2009
 */
public class TestGroup {
    private List<Path> tests;
    private List<Path> negativeTests;
    private List<String> formulas;

    public TestGroup(List<Path> tests,
                     List<Path> negativeTests,
                     List<String> formulas) {
        this.tests = tests;
        this.negativeTests = negativeTests;
        this.formulas = formulas;
    }

    public List<Path> getTests() {
        return tests;
    }

    public List<Path> getNegativeTests() {
        return negativeTests;
    }

    public List<String> getFormulas() {
        return formulas;
    }
}
